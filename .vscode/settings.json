{
    "workbench.iconTheme": "material-icon-theme",
    "window.zoomLevel": 0,
    "explorer.confirmDelete": false,
    "files.autoSave": "off",
    "prettier.eslintIntegration": true,
    "eslint.autoFixOnSave": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        {
            "language": "html",
            "autoFix": true
        },
        {
            "language": "vue",
            "autoFix": true
        }
    ],
    "emmet.includeLanguages": {
        "edge": "html",
        "vue-html": "html",
          "vue": "html"
      },
}